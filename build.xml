<project name="tq3" basedir="." default="run">
	<import file="library-locator.xml"/>
	<path id="cp">
		<pathelement path="."/>
		<fileset dir="${dir.jgoodies}"><include name="**/*.jar"/></fileset> 
		<fileset dir="${dir.jdbc-drivers}"><include name="**/*.jar"/></fileset> 
		<fileset dir="${dir.jcalendar}"><include name="**/*.jar"/></fileset> 
	</path>
	<target name="run">
		<javac srcdir="." destdir="." debug="true" classpathref="cp">
			<include name="**/*.java"/>
		</javac>
		<java classpathref="cp" fork="true" classname="Tq3"/>
	</target>
</project>
